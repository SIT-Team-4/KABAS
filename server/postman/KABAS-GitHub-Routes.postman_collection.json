{
  "info": {
    "name": "KABAS - GitHub Routes",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000/api"
    },
    {
      "key": "githubToken",
      "value": "<your-github-token>"
    },
    {
      "key": "owner",
      "value": "your-org-or-user"
    },
    {
      "key": "repo",
      "value": "your-repo-name"
    }
  ],
  "item": [
    {
      "name": "Get Kanban Data",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "X-GitHub-Token",
            "value": "{{githubToken}}"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/github/{{owner}}/{{repo}}/kanban",
          "host": ["{{baseUrl}}"],
          "path": ["github", "{{owner}}", "{{repo}}", "kanban"]
        }
      }
    },
    {
      "name": "Get Kanban Data - No Token (401)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/github/{{owner}}/{{repo}}/kanban",
          "host": ["{{baseUrl}}"],
          "path": ["github", "{{owner}}", "{{repo}}", "kanban"]
        }
      }
    }
  ]
}